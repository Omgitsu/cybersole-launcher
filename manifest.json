{
  "manifest_version": 3,
  "name":    "Cybersole Launcher",
  "version": "0.2",
  "offline_enabled": false,
  "action": {
    "default_icon": "img/cyber-logo.png"
  },
  "icons": {
    "128": "img/cyber-logo.png"
  },
  "options_page": "options.html",
  "permissions": [
      "tabs",
      "storage",
      "notifications"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}